{
  "name": "vue",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "prepare": "husky",
    "lint-staged": "lint-staged",
    "lint:eslint": "eslint **/*.{js,ts,tsx,vue} --fix --quiet",
    "lint:stylelint": "stylelint \"*/**/*.{vue,css,scss}\"",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.19.0",
    "eslint-plugin-vue": "^9.32.0",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.2",
    "stylelint": "^16.14.0",
    "stylelint-config-recommended-vue": "^1.6.0",
    "stylelint-config-standard": "^37.0.0",
    "stylelint-order": "^6.0.4",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.21.0",
    "vite": "^6.0.5",
    "vue-tsc": "^2.2.0"
  },
  "lint-staged": {
    "*/**/*.{js,ts,tsx,vue}": [
      "eslint --fix"
    ],
    "*/**/*.{vue,css,scss}": [
      "stylelint"
    ]
  }
}
