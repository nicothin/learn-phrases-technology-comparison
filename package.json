{
  "name": "solid",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "prepare": "husky",
    "lint-staged": "lint-staged",
    "lint:eslint": "eslint **/*.{js,ts,tsx} --fix --quiet",
    "lint:stylelint": "stylelint \"*/**/*.css\"",
    "build": "tsc -b && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "solid-js": "^1.9.3"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "lint-staged": "^15.4.2",
    "stylelint": "^16.14.0",
    "stylelint-config-standard": "^37.0.0",
    "typescript": "~5.6.2",
    "vite": "^6.0.5",
    "vite-plugin-solid": "^2.11.0"
  },
  "lint-staged": {
    "*/**/*.{js,ts,tsx}": [
      "eslint --fix"
    ],
    "*/**/*.css": [
      "stylelint"
    ]
  }
}
